	СУММА(ТиповойОстатки.СуммаОстатокКт) КАК СуммаОстатокКт
{ВЫБРАТЬ
	Организация.*,
	Валюта.*,
	Субконто1.*,
	Субконто2.*,
	Субконто3.*,
	СуммаОстаток,
	СуммаОстатокДт,
	СуммаОстатокКт}
